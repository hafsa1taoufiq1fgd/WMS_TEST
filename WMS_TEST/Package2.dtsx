                <property
                  dataType="System.String"
                  description="Commande SQL à exécuter."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">/***** pour remplir la table Import attendue fournisseur, la jointure entre PORDER (en-tête de la commande d'achat) et PORDERQ (quantités de la commande d'achat) est effectuée. ****/

select
entet.POHFCY_0 as CODE_SITE,
entet.BPSNUM_0 as CODE_FRS ,
entet.POHNUM_0 as REFERANCE ,
ligne.EXTRCPDAT_0 as DATE_LIV /** Date réception prévue **/,
ligne.POPLIN_0 as NUM_LIGNE,
ligne.ITMREF_0 as ART_CODE,
ligne.QTYPUU_0 as QTE,
entet.UPDDATTIM_0 
from SBG.PORDER as entet JOIN SBG.PORDERQ as ligne
on entet.POHNUM_0=ligne.POHNUM_0 /** chaque commande avec ses articles correspondants **/
where
entet.POHFCY_0 in ('SFGD','SCDI' ,'NATT' ,'PMMT','SMMT','SEXP','SCMG','SONS','MHCP') /*** Site de réception **/
and ligne.ITMREF_0 in (
select ITMREF_0 from SBG.ITMFACILIT as Site
where Site.STOFCY_0 in ('SFGD','SCDI' ,'NATT' ,'PMMT','SMMT','SEXP','SCMG','SONS','MHCP')
and Site.STOMGTCOD_0=2
) /**** Les articles doivent être dans liste des articles qui sont gérés en stock sur les sites géré dans wms ****/
and CLEFLG_0=1 /* les commandes non soldées */
and XFLGW_0=2 /**** *****/
order by entet.POHNUM_0
/*** les Filtres par date seront traités au niveau SSIS **/
/*and entet.UPDDATTIM_0 &gt;DATEADD(hh, 00, DATEADD(dd, 0, 0)) */

/*and   entet.UPDDATTIM_0 &gt;DATEADD(hh, 00, DATEADD(dd, DATEDIFF(dd, 0, GETDATE()), 0))*/
/** Pour limiter les commandes de cree/update à la date actuelle.  */
 /*** pour organiser les commandes ***/</property>
